{
  "mcpServers": {
    "fal-ai": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "mcp",
        "--with",
        "fal-client",
        "python",
        "/absolute/path/to/fal-mcp-server/src/fal_mcp_server/server.py"
      ],
      "env": {
        "FAL_KEY": "your-fal-api-key-here"
      }
    }
  }
}
